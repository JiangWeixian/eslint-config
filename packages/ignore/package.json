{
  "name": "@aiou/eslint-ignore",
  "version": "0.5.2",
  "description": "collection of eslint ignore patterns",
  "keywords": [
    "eslint",
    "ignore"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JiangWeixian/eslint-config#readme",
